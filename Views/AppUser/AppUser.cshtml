@{
}

@model IEnumerable<StoreManagement.DTO.AppUserDTO>

@{
    ViewData["Title"] = "App Users";
}

<h2>App Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>User Name</th>  
            <!-- Add other columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Name</td>
                <!-- Add other columns as needed -->
            </tr>
        }
    </tbody>
</table>
